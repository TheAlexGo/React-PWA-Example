{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@components": [
        "./components/ui"
      ],
      "@languages/*": [
        "./languages/*"
      ],
      "@pages": [
        "./components/pages"
      ],
      "@store": [
        "./store"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "bem-cn-custom": [
        "./utils/bemCnCustom"
      ]
    },
    "downlevelIteration": true,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "useDefineForClassFields": true
  },
  "include": [
    "src"
  ]
}
